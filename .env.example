# Copy this file to .env (do not commit .env)

# Required for Gemini highlight selection
GEMINI_API_KEY=

# Optional: Gemini model name (default: gemini-2.0-flash). Prefer stable IDs (avoid *-latest if it 404s).
GEMINI_MODEL=gemini-2.0-flash

# Optional: Gemini REST API version. Default is v1.
# If you must, you can try v1beta, but some models may 404 there.
GEMINI_API_VERSION=v1

# Optional: enable debug logs for Gemini parsing/request issues (prints to server stderr)
HIGHLIGHT_DEBUG=0
